<div class="c-content-client-logos-1">
  <div class="c-logos">
    <div class="row">

      {% for item in items %}

        <div class="col-md-4 col-xs-6 c-logo c-logo-{{ item.count }}">
          <a href="{{ item.href }}">
            <img class="c-img-pos" src="{{ item.src }}" width="180" height="140" alt="Carousel item"/>
          </a>
        </div>

      {% endfor %}

    </div>
  </div>
</div>
