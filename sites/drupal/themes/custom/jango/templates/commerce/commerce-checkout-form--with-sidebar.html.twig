{#
/**
 * @file
 * Two column template for the checkout form.
 *
 * Available variables:
 * - form: The form.
 *
 * @ingroup themeable
 */
#}
<div class="layout-checkout-form clearfix">
  <div class="layout-region order-summary">
    <h2>{% trans %} Order Summary {% endtrans %}</h2>
    {{ form.sidebar }}
  </div>
  <div class="layout-region billing-information">
    <h2>{% trans %} Billing information {% endtrans %}</h2>
    {{ form|without('sidebar', 'actions') }}
  </div>
  <div class="layout-region layout-region-checkout-footer">
    {{ form.actions }}
  </div>
</div>
